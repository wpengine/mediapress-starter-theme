          "radius": true,
          "style": true,
          "width": true
        }
      }
    },
    "color": {
      "background": true,
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "defaultDuotone": false,
      "defaultPalette": false,
      "defaultGradients": false,
      "duotone": [],
      "gradients": [],
      "palette": [
        {
          "color": "#ffffff",
          "name": "Base",
          "slug": "base"
