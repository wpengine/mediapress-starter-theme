  "styles": {
    "blocks": {
      "core/avatar": {
        "border": {
          "radius": "90px"
        }
      },
      "core/button": {
        "variations": {
          "outline": {
            "border": {
              "color": "currentColor",
              "style": "solid",
              "width": "1px"
            },
            "color": {
              "gradient": "transparent none",
              "text": "currentColor"
            },
            "spacing": {
              "padding": {
                "bottom": "calc(0.6rem - 1px)",
                "left": "calc(1rem - 1px)",
                "right": "calc(1rem - 1px)",
                "top": "calc(0.6rem - 1px)"
              }
            }
          }
        }
      },
      "core/buttons": {
        "spacing": {
          "blockGap": "0.7rem"
        }
      },
      "core/categories": {
        "color": {
          "background": ""
        }
      },
      "core/columns": {
        "css": ".wp-block-columns.is-style-hero .wp-block-column:first-child {position: relative!important;z-index: 2!important;margin-right: -100px!important;}",
        "spacing": {
          "blockGap": ""
        }
      },
      "core/list": {
        "spacing": {
          "padding": {
            "left": "var(--wp--preset--spacing--10)"
          }
        }
      },
      "core/navigation": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "typography": {
          "fontSize": "14px",
          "fontWeight": "500",
          "textTransform": "uppercase"
        }
      },
      "core/post-author": {
        "typography": {
          "fontStyle": "normal",
          "fontWeight": "400"
        }
      },
      "core/post-author-name": {
        "color": {
          "text": "var(--wp--preset--color--accent)"
        },
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "color": {
              "text": "var(--wp--preset--color--accent)"
            },
            "typography": {
              "fontWeight": "700",
              "textDecoration": "none"
            }
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "core/post-content": {
        "css": ".wp-block-post-content a:where(:not(.wp-element-button)) { text-underline-offset: .2em; }",
        "typography": {
          "lineHeight": "1.6"
        }
      },
      "core/post-date": {
        "color": {
          "text": "var(--wp--preset--color--contrast)"
        },
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "color": {
              "text": "var(--wp--preset--color--contrast)"
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "core/post-excerpt": {
        "typography": {
          "lineHeight": "1.6"
        }
      },
      "core/post-featured-image": {
        "border": {
          "radius": "var(--wp--preset--spacing--20)"
        }
      },
      "core/post-template": {
        "spacing": {
          "blockGap": ""
        }
      },
      "core/post-terms": {
        "border": {
          "radius": "var(--wp--preset--spacing--20)"
        },
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        },
        "css": "& .wp-block-post-terms__prefix{color: var(--wp--preset--color--contrast-2);}"
      },
      "core/post-title": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        }
      },
      "core/pullquote": {
        "typography": {
          "fontSize": "1.5em",
          "lineHeight": "1.6"
        }
      },
      "core/query-no-results": {
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--30)"
          }
        }
      },
      "core/query-title": {
        "css": "& span {font-style: italic;}"
      },
      "core/quote": {
        "border": {
          "left": {
            "color": "var(--wp--preset--color--accent)",
            "style": "solid",
            "width": "4px"
          }
        },
        "color": {
          "background": "var(--wp--preset--color--base-2)",
          "text": "var(--wp--preset--color--accent)"
        },
        "spacing": {
          "padding": {
            "bottom": "calc(var(--wp--preset--spacing--10) + 0.75rem)",
            "left": "calc(var(--wp--preset--spacing--20) + 0.75rem)",
            "right": "calc(var(--wp--preset--spacing--20) + 0.75rem)",
            "top": "calc(var(--wp--preset--spacing--10) + 0.75rem)"
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--heading)",
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontStyle": "italic",
          "lineHeight": "1.3"
        }
      },
      "core/separator": {
        "variations": {
          "wide": {
            "color": {
              "background": "#e9e9e9"
            }
          }
        }
      },
      "core/site-logo": {
        "variations": {
          "rounded": {
            "border": {
              "radius": "9999px"
            }
          }
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--base)",
      "text": "var(--wp--preset--color--contrast)"
    },
    "css": ":where(.wp-site-blocks *:focus){outline-width:2px;outline-style:solid}\n\n.taxonomy-category {\nborder-radius:20px;\n}",
    "elements": {
      "button": {
        ":active": {
          "color": {
            "background": "var(--wp--preset--color--contrast)",
            "text": "var(--wp--preset--color--base)"
          }
        },
        ":focus": {
          "border": {
            "color": "var(--wp--preset--color--contrast-2)"
          },
          "color": {
            "background": "var(--wp--preset--color--contrast-2)",
            "text": "var(--wp--preset--color--base)"
          },
          "outline": {
            "color": "var(--wp--preset--color--contrast)",
            "offset": "2px"
          }
        },
        ":hover": {
          "border": {
            "color": "var(--wp--preset--color--contrast-2)"
          },
          "color": {
            "background": "var(--wp--preset--color--contrast-3)",
            "text": "var(--wp--preset--color--base)"
          }
        },
        "border": {
          "color": "var(--wp--preset--color--contrast)",
          "radius": ".33rem",
          "width": "0"
        },
        "color": {
          "background": "var(--wp--preset--color--accent)",
          "text": "var(--wp--preset--color--base)"
        },
        "spacing": {
          "padding": {
            "bottom": "0.6rem",
            "left": "1rem",
            "right": "1rem",
            "top": "0.6rem"
          }
        },
        "typography": {
          "fontFamily": "inherit",
          "fontSize": "var(--wp--preset--font-size--small)",
